class Solution {
    public int minOperations(int[] nums) {
        int n = nums.length;

        // Step 1: If the array has 1, just count
        int ones = 0;
        for (int x : nums) if (x == 1) ones++;
        if (ones > 0) return n - ones;

        // Step 2: Otherwise, find the shortest subarray with GCD=1
        int minLen = Integer.MAX_VALUE;
        for (int i = 0; i < n; i++) {
            int g = nums[i];
            for (int j = i + 1; j < n; j++) {
                g = gcd(g, nums[j]);
                if (g == 1) {
                    minLen = Math.min(minLen, j - i + 1);
                    break;
                }
            }
        }

        // Step 3: If no subarray with GCD=1, return -1
        if (minLen == Integer.MAX_VALUE) return -1;

        // Step 4: Result = operations to create 1 + propagation
        return (minLen - 1) + (n - 1);
    }

    private int gcd(int a, int b) {
        while (b != 0) {
            int t = a % b;
            a = b;
            b = t;
        }
        return a;
    }
}
